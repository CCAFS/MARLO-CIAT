<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.cgiar.ccafs.marlo.data.model.OutputNextUser"
		table="output_nextusers" optimistic-lock="version">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="identity" />
		</id>
		  <property name="name" type="string">
      <column name="name" length="65535" not-null="true" />
    </property>
    <set name="researchOutputs" table="research_output_nextusers" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="nextuser_id" not-null="true" />
            </key>
            <many-to-many entity-name="org.cgiar.ccafs.marlo.data.model.ResearchOutput">
                <column name="research_output_id" not-null="true" />
            </many-to-many>
        </set>
	</class>
</hibernate-mapping>
