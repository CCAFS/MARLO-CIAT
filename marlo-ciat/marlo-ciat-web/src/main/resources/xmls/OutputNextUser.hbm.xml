<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.cgiar.ccafs.marlo.data.model.OutputNextUser"
		table="output_nextusers" optimistic-lock="version">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="identity" />
		</id>
		  <property name="active" type="boolean">
      <column name="is_active" not-null="true" />
    </property>
    <property name="activeSince" type="timestamp">
      <column name="active_since" length="19" not-null="true" />
    </property>
    <many-to-one name="createdBy" class="org.cgiar.ccafs.marlo.data.model.User"
      fetch="select">
      <column name="created_by" not-null="false" />
    </many-to-one>
    <many-to-one name="modifiedBy" class="org.cgiar.ccafs.marlo.data.model.User"
      fetch="select">
      <column name="modified_by" not-null="false" />
    </many-to-one>
    <property name="modificationJustification" type="string">
      <column name="modification_justification" length="65535"
        not-null="false" />
    </property>
		  <property name="name" type="string">
      <column name="name" length="65535" not-null="true" />
    </property>
    <set name="researchOutputs" table="research_output_nextusers" inverse="false" lazy="true" fetch="select" cascade="all">
            <key>
                <column name="nextuser_id"/>
            </key>
            <many-to-many entity-name="org.cgiar.ccafs.marlo.data.model.ResearchOutput">
                <column name="research_output_id" not-null="true" />
            </many-to-many>
        </set>
	<set name="nextOutputSubUsers" table="output_nextsubuser"
		inverse="true" lazy="true" fetch="select">
		<key>
			<column name="next_userid" />
		</key>
		<one-to-many class="org.cgiar.ccafs.marlo.data.model.OutputNextSubUser" />
	</set>
	</class>
</hibernate-mapping>
