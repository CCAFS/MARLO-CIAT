<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

	<package name="capDev" namespace="/capdev" extends="marlo-default">

		<action name="{center}/capdev"
			class="org.cgiar.ccafs.marlo.action.capdev.CapacityDevelopmentAction"
			method="list">
			<result type="freemarker" name="success">/WEB-INF/views/capDev/trainings.ftl
			</result>
		</action>

		<action name="{center}/addCapdev"
			class="org.cgiar.ccafs.marlo.action.capdev.CapacityDevelopmentAction"
			method="add">
			<result name="input">/WEB-INF/views/capDev/trainings.ftl
			</result>
			<result name="success" type="redirectAction">
				<param name="actionName">${centerSession}/detailCapdev</param>
				<param name="projectID">${projectID}</param>

			</result>
		</action>

		<action name="{center}/detailCapdev"
			class="org.cgiar.ccafs.marlo.action.capdev.CapacityDevelopmentDescripcionAction">
			<result name="input">/WEB-INF/views/capDev/detailCapdev.ftl
			</result>
			<result name="success" type="redirectAction">
				<param name="actionName">${centerSession}/detailCapdev</param>
				<param name="capDevID">${capDevID}</param>
				<param name="edit">true</param>
			</result>
		</action>


		<action name="{center}/addDeliverable"
			class="org.cgiar.ccafs.marlo.action.capdev.CapDevDeliverablesAction">
			<result name="input">/WEB-INF/views/capDev/newDeliverable.ftl
			</result>
			<result type="freemarker" name="success">/WEB-INF/views/capDev/newDeliverable.ftl
			</result>
		</action>



		







	</package>

</struts>