<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN" "http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>

	<package name="capDev" namespace="/capdev" extends="marlo-default">

		<action name="{center}/capdev"
			class="org.cgiar.ccafs.marlo.action.capdev.CapacityDevelopmentAction"
			method="list">
			<result type="freemarker" name="success">/WEB-INF/views/capDev/trainings.ftl
			</result>

		</action>

		<action name="{center}/addCapdev"
			class="org.cgiar.ccafs.marlo.action.capdev.CapacityDevelopmentAction"
			method="add">
			<result name="success" type="freemarker">/WEB-INF/views/capDev/newCapdev.ftl
			</result>
		</action>

	</package>

</struts>